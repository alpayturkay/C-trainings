#include<stdio.h>

int main(){

int n[10]={7,8,9,15,27,36,48,24,99,11};
int i,search,found=0;

printf("Enter the number that you want to find:");
scanf("%d",&search);
for(i=0;i<10;i++){if(n[i]==search){found=1;
break;}
}
   if(found==1){
    printf("Found.");
   }
else
    printf("Not found.");

return 0;
}
