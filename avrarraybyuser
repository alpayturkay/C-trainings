//Average of the array entered by user

#include<stdio.h>

int main(){
int n;
float sum = 0.0;
float ar[5];
int i;
printf("Enter how many numbers u gonna enter:");
scanf("%d",&n);
for(i=0;i<n;i++){
printf("Enter an number:");
scanf("%f",&ar[i]);
}
for(i=0;i<5;i++){
sum +=ar[i];
}

printf("Average %.2f",sum/5);


return 0;
}
